{
    "ColorStyles": {
        "body": {
            "Type": "CSS",
            "color": {
                "Desc": "Body Fore-Color",
                "Value": "#FFFFFF"
            },
            "background-color": {
                "Desc": "Body Back-Color",
                "Value": "#1F1F1F"
            }
        },
        "#icon": {
            "Type": "CSS",
            "color": {
                "Desc": "Sidebar Icon Fore-Color",
                "Value": "#FFFFFF"
            },
            "background-color": {
                "Desc": "Sidebar Icon Back-Color",
                "Value": "#2F2F2F"
            }
        },
        "#text": {
            "Type": "CSS",
            "color": {
                "Desc": "Sidebar Info Fore-Color",
                "Value": "#FFFFFF"
            },
            "background-color": {
                "Desc": "Sidebar Info Back-Color",
                "Value": "#2F2F2F"
            }
        },
        "#credits": {
            "Type": "CSS",
            "background-color": {
                "Desc": "Credits Button Back-Color",
                "Value": "#2F2F2F"
            }
        },
        "#credits a": {
            "Type": "CSS",
            "color": {
                "Desc": "Credits Button Fore-Color",
                "Value": "#4F4F4F"
            }
        },
        "#infodiv": {
            "Type": "CSS",
            "background-color": {
                "Desc": "Sidebar Back-Color",
                "Value": "#2F2F2F"
            }
        },
        "#topleft": {
            "Type": "CSS",
            "background-color": {
                "Desc": "Top Bar Left Back-Color",
                "Value": "#2F2F2F"
            },
            "border-right": {
                "Desc": "Top Bar Left, Right Border",
                "Value": "1px solid #1F1F1F"
            }
        },
        "#settingsicon button img": {
            "Type": "CSS",
            "background-color": {
                "Desc": "Settings Icon Back Color",
                "Value": "#2F2F2F"
            }
        },
        "#topright": {
            "Type": "CSS",
            "background-color": {
                "Desc": "Top Bar Right Back-Color",
                "Value": "#2F2F2F"
            },
            "color": {
                "Desc": "Top Bar Right Fore-Color",
                "Value": "#6F6F6F"
            }
        },
        "tr.indexhead a": {
            "Type": "CSS",
            "color": {
                "Desc": "Table Header Link Fore-Color",
                "Value": "#FFFFFF"
            }
        },
        "tr.indexhead": {
            "Type": "CSS",
            "border-bottom": {
                "Desc": "Table Header Bottom Border",
                "Value": "1px solid #2F2F2F"
            },
            "background-color": {
                "Desc": "Table Header Back-Color",
                "Value": "#1F1F1F"
            }
        },
        "tr.odd": {
            "Type": "CSS",
            "color": {
                "Desc": "Table Row Odd Fore-Color",
                "Value": "#FFFFFF"
            },
            "background-color": {
                "Desc": "Table Row Odd Back-Color",
                "Value": "#4F4F4F"
            }
        },
        "tr.even": {
            "Type": "CSS",
            "color": {
                "Desc": "Table Row Even Fore-Color",
                "Value": "#FFFFFF"
            },
            "background-color": {
                "Desc": "Table Row Even Back-Color",
                "Value": "#1F1F1F"
            }
        },
        "tr.odd a": {
            "Type": "CSS",
            "color": {
                "Desc": "Table Row Odd Link Fore-Color",
                "Value": "#FFFFFF"
            }
        },
        "tr.even a": {
            "Type": "CSS",
            "color": {
                "Desc": "Table Row Even Link Fore-Color",
                "Value": "#FFFFFF"
            }
        },
        "::-webkit-scrollbar": {
            "Type": "CSS",
            "width": {
                "Desc": "WebKit Scrollbar Width",
                "Value": "4px"
            }
        },
        "::-webkit-scrollbar-thumb": {
            "Type": "CSS",
            "background": {
                "Desc": "WebKit Scrollbar Back-Color",
                "Value": "#888888"
            },
            "border": {
                "Desc": "WebKit Scrollbar Border",
                "Value": "thin solid #808080"
            },
            "border-radius": {
                "Desc": "WebKit Scrollbar Border Radius",
                "Value": "10px"
            }
        },
        "::-webkit-scrollbar-thumb:hover": {
            "Type": "CSS",
            "background": {
                "Desc": "WebKit Scrollbar Hover Back-Color",
                "Value": "#666666"
            },
            "border": {
                "Desc": "WebKit Scrollbar Hover Border",
                "Value": "thin solid #666666"
            }
        },
        ".rcrumbs *": {
            "Type": "CSS",
            "color": {
                "Desc": "Breadcrumb Fore-Color",
                "Value": "#6F6F6F"
            }
        },
        ".rcrumbs .divider": {
            "Type": "CSS",
            "color": {
                "Desc": "Breadcrumb Divider Fore-Color",
                "Value": "#CCCCCC"
            }
        },
        "#settings": {
            "Type": "CSS",
            "background-color": {
                "Desc": "Settings Back-Color",
                "Value": "#1F1F1F"
            },
            "color": {
                "Desc": "Settings Fore-Color",
                "Value": "#6F6F6F"
            }
        },
        "QRCODE": {
            "Type": "JS",
            "color": {
                "Desc": "QR Code Fore-Color",
                "Value": "#FFFFFF"
            },
            "background-color": {
                "Desc": "QR Code Back-Color",
                "Value": "#2F2F2F"
            }
        }
    },
    "GlobalSettings": {
        "EnableVideoPlayer": {
            "Desc": "Enable Video Player?",
            "Value": true
        },
        "EnableAudioPlayer": {
            "Desc": "Enable Audio Player?",
            "Value": true
        },
        "EnablePhotoViewer": {
            "Desc": "Enable Photo Viewer?",
            "Value": true
        }
    }
}

